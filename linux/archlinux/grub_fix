# Boot from archlinux iso.

cd ..
# Linux file system.
mount /dev/nvme0n1p6 /mnt

# Efi.
mount /dev/nvme0n1p1 /mnt/efi

arch-chroot /mnt

grub-install --target=x86_64-efi --efi-directory=/efi --bootloader-id=GRUB

grub-mkconfig -o /boot/grub/grub.cfg

# Boot from linux system.

grub-install --target=x86_64-efi --efi-directory=/efi --bootloader-id=GRUB

grub-mkconfig -o /boot/grub/grub.cfg