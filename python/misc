# Query an object.
dir(time)

# Asking the interpreter for help.
help(time)
help(time.strftime)

# Get a value from user.
a = input("Entry a value: ")

# Multiplication operator.
# asdf asdf asdf 
print('asdf '*3)

# Get the type.
type(object)

# Pretty print.
import pprint
pprint.pprint(some_complex_dictionary)

# Tell if the value evaluetes to true or false.
bool(0)
bool('')
bool([])
