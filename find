find .     # lista todos os caminhos de todos arquivos do diretório corrente

find . -name "*js*"    # using wildcard

find / -name <file_name>     # procura por file_name a partir do diretório raiz case sensitive

find / -iname <file_name>    # procura por file_name a partir do diretório raiz case insensitive

find /tmp /var/tmp . $HOME -name 2>/dev/null     # procura em vários lugares e não imprime erro

find / -type f -mtime -7     # modificados até 7 dias

find / -mmin -10       # 10 minutos atrás

find / -name <name> -type d      # find directory

find -maxdepth 2 -name <name>    # depth

find / -group group-name    # list files owned by a group

find / -user user-name      # list files owned by a user
