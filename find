# lista todos os caminhos de todos arquivos do diretório corrente
find . 

# using wildcard
find . -name "*js*"

# procura por file_name a partir do diretório raiz case sensitive
find / -name <file_name>

# procura por file_name a partir do diretório raiz case insensitive
find / -iname <file_name>

# procura em vários lugares e não imprime erro
find /tmp /var/tmp . $HOME -name 2>/dev/null

# modificados até 7 dias
find / -type f -mtime -7

# 10 minutos atrás
find / -mmin -10

# find directory
find / -name <name> -type d
